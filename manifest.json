{
    "name": "ResumePuppy",
    "version": "1.0",
    "description": "Build an Extension!",
    "permissions": ["activeTab","storage","declarativeContent"],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
      },
      "page_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/icon.png",
            "32": "images/icon.png",
            "48": "images/icon.png",
            "128": "images/icon.png"
          }
      },
      "icons": {
        "16": "images/icon.png",
          "32": "images/icon.png",
          "48": "images/icon.png",
          "128": "images/icon.png"
      },
      "content_scripts": [
        {
          "matches": ["https://www.linkedin.com/jobs/view/*","http://www.linkedin.com/jobs/view/*"],
          "run_at": "document_idle",
          "js": ["app.js"]
        }
      ],
    "manifest_version": 2
  }